package com.java.jdk8;

public class Calculation {
	
	public static void main(String[] args) {
		
		Icalculation obj = (a,b) -> {
			return a+b;
		};
		
		Icalculation obj1 = (a,b) -> {
			return a-b;
		};
		
		Icalculation obj2 = (a,b) -> {
			return a*b;
		};
		
		System.out.println("Sum is : "+ obj.cal(1, 5));
		System.out.println("sub is : "+obj1.cal(12, 5));
		System.out.println("Mul is : "+obj2.cal(12, 5));
	}

}
