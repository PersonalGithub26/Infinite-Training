package com.java.multiThreading;

import javax.sound.midi.Synthesizer;

public class Even extends Thread{
	
	@Override
	public void run() {
		for(int i =0; i<= 20; i+=2)
		{
			System.out.println("EVen "+i);
		}
		try{
			Thread.sleep(100);
		}catch(InterruptedException e)
		{
			e.getMessage();
		}
	}
	
	

}
