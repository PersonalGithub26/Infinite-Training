package com.java.jdk8;

import java.util.ArrayList;
import java.util.List;

public class MainEachEmploy {
	
	
	public static void main(String[] args) {
		
		List<ForEachMethod> employList = new ArrayList<ForEachMethod>();
		employList.add(new ForEachMethod(1, "Chandu", 7635));
		employList.add(new ForEachMethod(1, "ajay", 7635));
		employList.add(new ForEachMethod(1, "srinivas", 7635));
		employList.add(new ForEachMethod(1, "srikanth", 7635));
		employList.add(new ForEachMethod(1, "pavan", 7635));
		
		System.out.println("Employ List is :");
		employList.forEach(x -> {
			System.out.println(x);
		});
	}

}
