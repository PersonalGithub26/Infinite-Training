package com.java.abs;

import com.java.abs1.IPhone;
import com.java.abs1.Mobile;
import com.java.abs1.MobileDetails;
import com.java.abs1.Samsung;

public class MobileFactory implements MobileDetails{

	@Override
	public Mobile getMobileDetails(String value) {
		if(value.equals("Iphone"))
		{
			return new IPhone();
		}
		if(value.equals("samsung"))
		{
			return new Samsung();
		}
		if(value.equals("Iphone"))
		{
			return new IPhone();
		}
		if(value.equals("Iphone"))
		{
			return new IPhone();
		}
		return null;
	}

}
