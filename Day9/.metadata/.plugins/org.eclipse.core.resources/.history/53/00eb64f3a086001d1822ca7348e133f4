package practice;

public class P13 {
    public static void main(String[] args) {
        try {
            int a=5;
            int b=0;
            int c=b/a;
            System.out.println("Hi");
        } catch(ArithmeticException e) {
            System.out.println("Divex");
        } finally {
            System.out.println("Program End...");
        }
        System.out.println("Program from FTP114");
    }
}
